<% form_for :acquisition, @acquisition, :url => acquisition_order_path, :html => { :method => :put, :id => @acquisition, :multipart => true } do |f| %>
 <%= render :partial => 'form', :locals => {:f => f}%>
 <div id="row_submit">
   <%= f.submit "Actualizar datos", :onclick => "return (confirm('¿Desea actualizar los datos?'));"%>
   <%= link_to 'Cancelar', acquisition_orders_path, :confirm => 'Al cancelar los cambios no serán guardados' %>
 </div>
<% end%>
